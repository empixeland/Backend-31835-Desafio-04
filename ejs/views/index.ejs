<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Document</title>
  </head>
  <body>
    <ul >
      <li >
        <a href="/formulario" >Form</a>
      </li>
      <li>
        <a href="/productos" >Products</a>
      </li>
    </ul>
    <div >
      <table>
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Thumbnail</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% data.forEach((item)=>{ %>
            <td><%= item.title %></td>
            <td><%= item.price %></td>
            <td>
              <img
                src="<%=item.thumbnail%>"
                alt="<%=item.title%>"
                width="250"
                height="250"
              />
            </td>
          </tr>
          <%}) %>
        </tbody>
      </table>
    </div>
  </body>
</html>