      <div>
        <table>
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Price</th>
              <th scope="col">Thumbnail</th>
            </tr>
          </thead>
          <tbody>
            <tr>{{#each data}}
              <td>{{this.title}}</td>
              <td>{{this.price}}</td>
              <td><img src={{this.thumbnail}} alt={{this.title}} width="200" height="200"/></td>
            </tr>
              {{/each}}
        </table>
      </div>